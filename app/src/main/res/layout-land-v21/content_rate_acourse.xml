<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".rateACourse"
    tools:showIn="@layout/activity_rate_acourse">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="500dp">

        <SeekBar
            android:id="@+id/seekBar"
            style="@android:style/Widget.Material.SeekBar"
            android:layout_width="240dp"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="30dp"
            android:max="100"
            android:min="1"
            android:progress="50" />

        <Button
            android:id="@+id/button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/seekBar7"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="188dp"
            android:onClick="rateCourse"
            android:text="@string/rate" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="24dp"
            android:layout_below="@id/textView9"
            android:layout_marginTop="11dp"
            android:layout_marginEnd="7dp"
            android:layout_toStartOf="@id/seekBar"
            android:gravity="center"
            android:text="@string/subject_relevance"
            android:textSize="12sp" />

        <SeekBar
            android:id="@+id/seekBar2"
            style="@android:style/Widget.Material.SeekBar"
            android:layout_width="240dp"
            android:layout_height="wrap_content"
            android:layout_below="@id/seekBar"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="30dp"
            android:max="100"
            android:min="1"
            android:progress="50" />

        <SeekBar
            android:id="@+id/seekBar3"
            style="@android:style/Widget.Material.SeekBar"
            android:layout_width="240dp"
            android:layout_height="wrap_content"
            android:layout_below="@id/seekBar2"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="30dp"
            android:max="100"
            android:min="1"
            android:progress="50" />

        <SeekBar
            android:id="@+id/seekBar4"
            style="@android:style/Widget.Material.SeekBar"
            android:layout_width="240dp"
            android:layout_height="wrap_content"
            android:layout_below="@id/seekBar3"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="30dp"
            android:max="100"
            android:min="1"
            android:progress="50" />

        <SeekBar
            android:id="@+id/seekBar5"
            style="@android:style/Widget.Material.SeekBar"
            android:layout_width="240dp"
            android:layout_height="wrap_content"
            android:layout_below="@id/seekBar4"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="30dp"
            android:max="100"
            android:min="1"
            android:progress="50" />

        <SeekBar
            android:id="@+id/seekBar6"
            style="@android:style/Widget.Material.SeekBar"
            android:layout_width="240dp"
            android:layout_height="wrap_content"
            android:layout_below="@id/seekBar5"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="30dp"
            android:max="100"
            android:min="1"
            android:progress="50" />

        <SeekBar
            android:id="@+id/seekBar7"
            style="@android:style/Widget.Material.SeekBar"
            android:layout_width="240dp"
            android:layout_height="wrap_content"
            android:layout_below="@id/seekBar6"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="30dp"
